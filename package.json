{
  "name": "@cloudgraph/sdk",
  "version": "0.3.4",
  "description": "SDK for cloud graph providers and cli",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "homepage": "https://www.cloudgraph.dev/",
  "repository": "github:cloudgraphdev/sdk",
  "bugs": "https://github.com/cloudgraphdev/sdk/issues",
  "scripts": {
    "build": "yarn prepublish",
    "prepublish": "npx tsc",
    "lint": "eslint --config .eslintrc.json --ext .js,.ts ./",
    "lint:fix": "eslint --fix --config .eslintrc.json --ext .js,.ts ./"
  },
  "author": "AutoCloud",
  "license": "MPL-2.0",
  "dependencies": {
    "@types/node": "^16.4.12",
    "inquirer": "^8.1.2",
    "ora": "^5.4.1",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@autocloud/eslint-config": "^0.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/gitlab": "^6.2.2",
    "@semantic-release/npm": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^4.3.0",
    "lint-staged": "^11.1.1",
    "semantic-release": "^17.4.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,graphql,json}": [
      "yarn lint:fix",
      "git add --force"
    ]
  }
}
